--exclude=.git
--exclude=resources/*
--exclude=*/resources/*
--exclude=node_modules/*
--exclude=*/node_modules/*
--exclude=*/public/*
--exclude=.repl/*
--exclude=*/.repl/*
--exclude=out/*
--exclude=*/out/*
--exclude=target/*
--exclude=*/target/*
--exclude=*min.js
--exclude=*.js
--langdef=Clojure
--langmap=Clojure:.clj.cljs
--regex-clojure=/\([ \t]*create-ns[ \t]+([-[:alnum:]*+!_><:\/.?]+)/\1/n,namespace/
--regex-clojure=/\([ \t]*def[ \t]+([\^:[:alnum:]]+)\ ([-[:alnum:]*+!_><:\/.?]+)/\2/d,define/
--regex-clojure=/\([ \t]*defui[ \t]+([\^:[:alnum:]]+)\ ([-[:alnum:]*+!_><:\/.?]+)/\2/u,defui/
--regex-clojure=/\([ \t]*defn-?[ \t]+([-[:alnum:]*+!_><:\/.?]+)/\1/f,function/
--regex-clojure=/\([ \t]*defmacro[ \t]+([-[:alnum:]*+!_><:\/.?]+)/\1/m,macro/
--regex-clojure=/\([ \t]*definline[ \t]+([-[:alnum:]*+!_><:\/.?]+)/\1/i,inline/
--regex-clojure=/\([ \t]*defmulti[ \t]+([-[:alnum:]*+!_><:\/.?]+)/\1/a,multimethod definition/
--regex-clojure=/\([ \t]*defmethod[ \t]+([-[:alnum:]*+!_><:\/.?]+)\ '?([-[:alnum:]*+!_><:\/.?]+)/\2/b,multimethod instance/
--regex-clojure=/\([ \t]*defonce[ \t]+([-[:alnum:]*+!_><:\/.?]+)/\1/c,define (once)/
--regex-clojure=/\([ \t]*defstruct[ \t]+([-[:alnum:]*+!_><:\/.?]+)/\1/s,struct/
--regex-clojure=/\([ \t]*intern[ \t]+([-[:alnum:]*+!_><:\/.?]+)/\1/v,intern/
--regex-clojure=/\([ \t]*ns[ \t]+([-[:alnum:]*+!_><:\/.?]+)/\1/n,namespace/
